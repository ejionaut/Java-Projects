                                    <TextField />
                                 </children>
                              </VBox>
                              <TextField />
                           </children>
                        </HBox>
                        <TextArea prefHeight="126.0" prefWidth="390.0" />
                        <HBox prefHeight="39.0" prefWidth="390.0">
                           <children>
                              <Button mnemonicParsing="false" text="Button" />
                              <Button mnemonicParsing="false" text="Button" />
                           </children>
                        </HBox>
                     </children>
                  </VBox>
               </children>
            </VBox>
