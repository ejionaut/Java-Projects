<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="mainStage" prefHeight="852.0" prefWidth="393.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.RegisterController">
   <children>
      <ImageView fitHeight="279.0" fitWidth="392.0" pickOnBounds="true" preserveRatio="true" />
      <TextArea fx:id="errorPrompt" editable="false" prefHeight="32.0" prefWidth="393.0" />
      <TextArea fx:id="userTextField" prefHeight="32.0" prefWidth="393.0" text="Username" />
      <TextArea fx:id="passwordTextField" prefHeight="32.0" prefWidth="393.0" text="Password" />
      <TextArea fx:id="cpasswordTextField" prefHeight="32.0" prefWidth="393.0" text="Confirm Password" />
      <Button fx:id="createBtn" mnemonicParsing="false" onAction="#createAccount" text="Create Account" />
      <Button fx:id="returnBtn" mnemonicParsing="false" onAction="#returnLogin" text="Cancel" />
   </children>
</VBox>
